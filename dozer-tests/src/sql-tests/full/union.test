control sortmode rowsort

# actor
statement ok
CREATE TABLE actor(
    actor_id integer NOT NULL,
    first_name text NOT NULL,
    last_name text,
    last_update text
)

# film_actor
statement ok
CREATE TABLE film_actor (
    actor_id integer NOT NULL,
    film_id integer NOT NULL,
    last_update timestamp NOT NULL
);


