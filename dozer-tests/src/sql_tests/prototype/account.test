query IRR
SELECT 
    account_name, 
    SUM(a.balance) AS total_balance, 
    SUM(t.amount) AS total_income
FROM 
    transactions t 
    JOIN accounts a ON t.account_id = a.account_id 
WHERE t.amount > 0
GROUP BY account_name



