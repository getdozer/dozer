              }
            },
            "from_block": {
              "type": [
                "integer",
                "null"
              ],
              "format": "uint64",
              "minimum": 0.0
            },
            "to_block": {
              "type": [
                "integer",
                "null"
              ],
              "format": "uint64",
              "minimum": 0.0
            },
            "topics": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        },
        "EthLogConfig": {
          "type": "object",
          "required": [
            "wss_url"
          ],
          "properties": {
            "contracts": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/EthContract"
