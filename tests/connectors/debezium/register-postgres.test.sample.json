{
  "name": "dozer-postgres-connector",
  "config": {
    "name": "dozer-postgres-connector",
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "tasks.max": "1",
    "database.hostname": "${KAFKA_POSTGRES_HOST}",
    "database.port": "${KAFKA_POSTGRES_PORT}",
    "database.user": "${KAFKA_POSTGRES_USER}",
    "database.password": "${KAFKA_POSTGRES_PASSWORD}",
    "database.dbname": "${KAFKA_POSTGRES_DATABASE}",
    "database.server.name": "dbserver1",
    "topic.prefix": "dbserver1",
    "schema.include.list": "${KAFKA_POSTGRES_SCHEMA}"
  }
}